    <skos:prefLabel xml:lang="fr">georennesmetropole</skos:prefLabel>
    <skos:scopeNote xml:lang="fr">Ce mot-clé permet de connaître les données publiées sur GéoRennesMétropole.</skos:scopeNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.metropole.rennes.fr/thesaurus/sig/rm_diffusion/urbavue/">
    <skos:inScheme rdf:resource="http://www.metropole.rennes.fr/thesaurus/sig/rm_diffusion"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="fr">urbavue</skos:prefLabel>
    <skos:scopeNote xml:lang="fr">Ce mot-clé permet de connaître les données utilisées par l'application Urbavue.</skos:scopeNote>
  </rdf:Description>
</rdf:RDF>
